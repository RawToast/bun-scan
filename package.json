{
  "name": "bun-scan",
  "version": "1.1.0-beta.1",
  "private": true,
  "description": "Vulnerability scanner for Bun projects",
  "keywords": [
    "audit",
    "bun",
    "dependencies",
    "osv",
    "scanner",
    "security",
    "security-scanner",
    "vulnerability",
    "vulnerability-scanner"
  ],
  "homepage": "https://github.com/RawToast/bun-scan",
  "bugs": {
    "url": "https://github.com/RawToast/bun-scan/issues"
  },
  "license": "MIT",
  "author": "rawtoast",
  "repository": {
    "type": "git",
    "url": "https://github.com/RawToast/bun-scan"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/RawToast"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "README.md",
    "SECURITY.md",
    "!src/**/__tests__",
    "src/**/*.ts"
  ],
  "type": "module",
  "types": "./src/index.ts",
  "exports": "./src/index.ts",
  "scripts": {
    "dev": "turbo watch compile format lint",
    "lint": "turbo lint",
    "lint:check": "oxlint check src packages",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "compile": "turbo compile",
    "test": "turbo test",
    "clean": "rm -rf dist build node_modules/.cache",
    "check": "bun format && bun lint:check && turbo compile && turbo test",
    "prepare": "bunx lefthook install"
  },
  "dependencies": {
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@types/bun": "1.3.6",
    "@typescript/native-preview": "7.0.0-dev.20260114.1",
    "bun-types": "1.3.6",
    "lefthook": "2.0.15",
    "oxfmt": "0.24.0",
    "oxlint": "1.39.0",
    "turbo": "2.7.5"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.3.0"
}
